{
   "version": 2,
   "builds": [
     {
       "src": "backend-pride-land/core/wsgi.py",
       "use": "@vercel/python",
       "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
     },
     {
       "src": "backend-pride-land/build_files.sh",
       "use": "@vercel/static-build",
       "config": {
         "distDir": "static-build"
       }
     }
   ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "/static/$1"
     },
     {
       "src": "/(.*)",
       "dest": "core/wsgi.py"
     }
   ]
 }